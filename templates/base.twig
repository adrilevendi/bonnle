<body class="page-{% block body_class %}{% endblock %}" data-template="base.twig">


<div class="loading-container">
	<div class="loading-screen"></div>
</div>


<header class="header">
	{% block header %}

		{% include "header.twig" %}

	{% endblock %}
</header>

<div data-barba="wrapper">
	<main data-barba="container" data-barba-namespace="home">
		<div class="wrapper {{ sidebar_class }}">
			{% block content %}
				Sorry, no content
			{% endblock %}
		</div>
		{% if sidebar %}
			<aside class="layout-sidebar">
				{{ sidebar }}
			</aside>
		{% endif %}
	</main>
</div>


{% block footer %}
	<footer id="footer">
		{% include 'footer.twig' %}
	</footer>
	{{ function('wp_footer') }}
{% endblock %}


